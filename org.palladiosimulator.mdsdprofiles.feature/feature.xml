      <import plugin="org.modelversioning.emfprofile.application.registry" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.resources" version="3.8.1" match="greaterOrEqual"/>
      <import plugin="org.modelversioning.emfprofile.registry" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.emf.edit"/>
      <import plugin="org.eclipse.ui" version="3.104.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.emf.edit.ui"/>
      <import plugin="org.palladiosimulator.commons"/>
      <import plugin="org.eclipse.emf.ecore.edit"/>
      <import plugin="org.modelversioning.emfprofile.edit"/>
      <import plugin="org.palladiosimulator.mdsdprofiles" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.views.properties.tabbed" version="3.6.0" match="greaterOrEqual"/>
